{
  "name": "test",
  "description": "test",
  "author": "apades",
  "manifest_version": 3,
  "version": "0.0.1",
  "host_permissions": [
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "js": [
        "content.js"
      ],
      "run_at": "document_end",
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["react-refresh.js"],
      "matches": ["<all_urls>"]
    },
    {
      "resources": ["assets/icon.png"],
      "matches": ["<all_urls>"]
    }
  ]
}
